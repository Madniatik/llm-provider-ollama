{
  "name": "llm-provider-ollama",
  "display_name": "Ollama Provider",
  "version": "0.2.0",
  "description": "Full Ollama provider package with ServiceProvider, migrations, seeders, routes, and views. Includes 10 pre-configured models (Llama, Mistral, CodeLlama, Gemma, Phi, DeepSeek)",
  "author": "Bithoven Team",
  "license": "MIT",
  "repository": "https://github.com/bithoven/llm-provider-ollama",
  "homepage": "https://ollama.ai",
  "support": {
    "docs": "https://github.com/bithoven/llm-provider-ollama/wiki",
    "issues": "https://github.com/bithoven/llm-provider-ollama/issues"
  },
  "requires": {
    "llm-manager": ">=0.4.0"
  },
  "providers": [
    "Bithoven\\LLMProviderOllama\\OllamaServiceProvider"
  ],
  "autoload": {
    "psr-4": {
      "Bithoven\\LLMProviderOllama\\": "src/"
    }
  }
}
